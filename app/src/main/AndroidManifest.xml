<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.afei.camerademo">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/ThemeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".surfaceview.SurfaceCameraActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".textureview.TextureCameraActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".glsurfaceview.GLSurfaceCameraActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".surfaceview.SurfaceCamera2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".textureview.TextureCamera2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".glsurfaceview.GLSurfaceCamera2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".textview.TextViewDemo"
            android:screenOrientation="portrait" />
        <activity android:name=".eventbus.FirstActivity" />
        <activity android:name=".eventbus.SecondActivity" />
        <activity android:name=".screenadaptation.ScreenAdaptActivity" />
        <activity android:name=".handler.HandleTestActivity" />
        <activity android:name=".toptabbar.IndexActivity" />
        <activity
            android:name=".statusbarutil.StatusBarMainActivity"
            android:theme="@style/StatusTheme" />
        <activity
            android:name=".statusbarutil.ColorStatusBarActivity"
            android:theme="@style/StatusTheme" />
        <activity
            android:name=".statusbarutil.ImageStatusBarActivity"
            android:theme="@style/StatusTheme" />
        <activity
            android:name=".statusbarutil.ImageViewActivity"
            android:theme="@style/StatusTheme" />
        <activity
            android:name=".statusbarutil.UseInFragmentActivity"
            android:theme="@style/StatusTheme" />
        <activity
            android:name=".statusbarutil.SwipeBackActivity"
            android:theme="@style/StatusTheme" />
        <activity
            android:name=".statusbarutil.SwitchModeActivity"
            android:theme="@style/StatusTheme" />
        <activity
            android:name=".tbstest.TBSActivity"
            android:theme="@style/StatusTheme" />
        <activity android:name=".test.TestActivity" />
        <activity android:name=".recyclerview.RecyclerviewActivity" />
        <activity android:name=".rxjava.RxjavaTestActivity" />
        <activity android:name=".retrofit.RxJavaAndRetorfitActivity" />
        <activity android:name=".rxjava.TransOperatorActivity" />
        <activity android:name=".databinding.DatabindingTest" />
        <activity android:name=".databinding.BaseObservableTest"></activity>
    </application>

</manifest>